{
  "name": "node-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "db:reset": "sequelize db:drop > /dev/null 2>&1 || true && sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "start:dev": "ts-node-dev ./index.ts",
    "tsc": "tsc",
    "migrate": "sequelize db:migrate",
    "migrate:status": "sequelize db:migrate:status",
    "migrate:rollback": "sequelize db:migrate:undo",
    "migrate:refresh": "sequelize db:migrate:undo:all && migrate",
    "db:seed": "sequelize db:seed",
    "db:seed:all": "sequelize db:seed:all",
    "make:migration": "sequelize migration:generate",
    "make:model": "sequelize model:generate",
    "make:seed": "sequelize seed:generate"
  },
  "dependencies": {
    "@firebase/auth": "^0.13.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase": "^7.5.0",
    "pg": "^7.14.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1",
    "sequelize-typescript": "^1.0.0"
  },
  "devDependencies": {
    "@types/faker": "^4.1.8",
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.14",
    "@types/sequelize": "^4.28.8",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "1.19.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.2"
  }
}
